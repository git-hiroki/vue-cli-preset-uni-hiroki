{
    "source": "./src", 
    "destination": "./docs/api",
    "includes": [
      "\\.(js|jsx|vue)$"
    ],
    "excludes": [
      "(bundle\\.js|export\\.js)$"
    ],
    "index": "./README.md",  
    "package": "./package.json",
    "outputAST": true, 
    "plugins": [
      { "name": "esdoc-standard-plugin" },
      { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } }
    ]
  }